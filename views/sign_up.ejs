<html>
    <head>
        <meta name="viewport" content="width=device-width, height=device-height, minimum-scale=1.0, maximum-scale=1.0, initial-scale=1.0">
        <link rel="shortcut icon" href="/img/site_logo.ico">
        <link rel="stylesheet" href="/css/sign-up.css">
        <link rel="stylesheet" href="/css/fontstyle.css">
        <title>회원가입</title>
    </head>
    <body style="background-color: rgb(33, 33, 33);">
        <header>
            <h2>회원가입</h2>
        </header>
        <form id="form-new-account" action="" method="POST">
            <div class="input-box">
                <input id="username" type="text" name="username" placeholder="아이디" autocomplete="off">
                <label for="username">아이디</label>
            </div>
            <div class="button-box">
                <button id="button01" type="button">중복확인</button>
            </div>
            <div class="input-box">
                <input id="password" type="password" name="password" placeholder="비밀번호">
                <label for="password">비밀번호</label>
            </div>
            <div class="input-box">
                <input id="password-check" type="password" name="password-check" placeholder="비밀번호 확인">
                <label for="password-check">비밀번호 확인</label>
            </div>
            <div class="input-box">
                <input id="nickname" type="text" name="nickname" placeholder="닉네임" autocomplete="off">
                <label for="nickname">닉네임</label>
            </div>
            <div class="button-box">
                <button id="button02" type="button">중복확인</button>
            </div>
            <div class="input-box">
                <input id="github" type="text" name="github" placeholder="깃허브 아이디" autocomplete="off">
                <label for="github">깃허브 아이디(필수아님)</label>
            </div>
            <input type="submit" value="가입완료">
        </form>
    </body>
    <script src="/socket.io/socket.io.js"></script>
    <script src="https://code.jquery.com/jquery-latest.min.js"></script>
    <script src="/script/sign_up.js"></script>
    <script src="https://code.jquery.com/jquery-latest.js"></script>
</html>